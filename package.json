  "scripts": {
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "npm run prebuild:lambda && concurrently --kill-others \"tsc --project lambda --watch\" \"netlify-lambda serve functions\" ",
    "serve": "gatsby serve",
    "copy:lambda": "copyfiles -u 1 ./lambda/.env \"./lambda/node_modules/**/*\" ./functions",
    "prebuild:lambda": "mkdirp functions && netlify-lambda install && npm run copy:lambda && tsc --project lambda",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "npm run prebuild:lambda && netlify-lambda build functions",
    "production": "./build.env.sh && npm run build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "eslint": "eslint src --ext ts",
    "test": "npm run eslint"
  },
